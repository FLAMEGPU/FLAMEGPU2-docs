{
    "problemMatcher": [
        {
            "owner": "sphinx-with-severity",
            "pattern": [
                {
                    "regexp": "^(.*):(\\d+):\\s+(error|ERROR|warning|WARNING):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "message": 4
                }
            ]
        },
        {
            "owner": "sphinx-without-severity",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^(.*):(\\d+):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "message": 3
                }
            ]
        }
    ]
}